#define TITLEDISPLAYNAME "Microsoft Visual C++ 6.0 Starts Here"

setup
{
	general {MainWindowBackgroundBitmap:banner.bmp;
		PrimaryApplication:MVC6SH.EXE;
		ApplicationDisplayName:TITLEDISPLAYNAME;
		Company:Microsoft;
		MainWindowCaption:TITLEDISPLAYNAME;
		MainWindowBackgroundText:TITLEDISPLAYNAME;
		AddRemoveProgramsText: Microsoft Visual C++ 6.0 Starts Here;
		LicenseAgreement:media\license.txt;
		DatabaseLocation:Harddrive;
		DatabaseName:MVC6SH.MDB;
		MediaViewFileName:MVC6SH.m14;
		MediaSourcePath:Media;
		InstallActiveMovie:Yes;
		ActiveMovieSetup:AMovie.exe;
		ShowActiveMovieStep:No;
		MarissaUninstallKey:Marissa Viewer}

	registry{CompanyKey:Software\Microsoft;
		ProductKey:Microsoft Press;
		TitleKey:Microsoft Visual C++ 6.0 Starts Here;
		PrimaryLocationKey:MVC6SH;
		PrimaryLocationVersionKey:1.0;
		PrimaryLocationValueName:Location;
		MediaViewValueName:MediaView File Path;
		MediaPathValueName:Media Path;
		ListsPathValueName:List Path;
		DatabaseValueName:Database Name}
}

billboards
{
	1	{Bitmap:media\billbrd5.bmp; MinimumDisplayTime: 4}
	
}


configurations
{


	Typical		{Name:&Minimum;
			Description:Installs Microsoft Visual C++ 6.0 Starts Here. Requires up to 13 MB of disk space.;
			Bitmap:typical.bmp;
			Group:Install;
			Group:Main;
			Group:Fonts;
			Group:DLLS;
			Group:OCXs;
		//	Group:Labs
	}


	Complete	{Name:&Complete;
			Description:Installs Microsoft Visual C++ 6.0 Starts Here including example code files. Requires up to 16 MB of disk space.;
			Bitmap:complete.bmp;
			Group:Install;
			Group:Main;
			Group:Fonts;
			Group:DLLS;
			Group:OCXs;
			Group:Examples}

}
groups
{
	Install	{Name:Install; Description:Uncompressed files for the install process}
	Main	{Name:Main; Description:The primary group of files}
	Examples {Implicit:No; SourceDirectory:Examples; ReadOnly:-; Remove:Never; Destination:MVC6SH}
	
	//Labs	{Implicit:No; SourceDirectory:Labs; ReadOnly:-; Remove:Never; Destination:MVC6SH}
	Fonts	{}
	DLLs	{}
	OCXs	{}
	NotInstalled	{Description:Files that setup needs to register, but are not installed}
}

Install
{
	setup.exe	{Remove:Never}
	iwclean.exe	{Destination:JetstreamShared; Remove:Never}
	banner.bmp	{Destination:FilePrimary; Remove:Always}
	typical.bmp	{Destination:FilePrimary; Remove:Always}
	complete.bmp	{Destination:FilePrimary; Remove:Always}

}

Main
{
	MVC6SH.EXE	{Remove:Always}
	MVC6SH.CNT	{Remove:Always}
	MVC6SH.FTS	{Remove:Always}
	MVC6SH.GID	{Remove:Always}
	MVC6SH.HLP	{Remove:Always}
	MVC6SH.MDB	{Remove:Always; ReadOnly:-}
	README.TXT	{Remove:Always}

